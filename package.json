    "postcss": "^8.5.6",
    "sass-embedded": "^1.89.2",
    "stylelint": "^16.22.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
